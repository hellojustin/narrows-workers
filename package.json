{
  "name": "narrows-workers",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "dotenv -e .env.dev -- sst dev --stage dev",
    "deploy:dev": "dotenv -e .env.dev -- sst deploy --stage dev",
    "deploy:staging": "dotenv -e .env.staging -- sst deploy --stage staging",
    "deploy:production": "dotenv -e .env.production -- sst deploy --stage production",
    "remove:dev": "dotenv -e .env.dev -- sst remove --stage dev",
    "remove:staging": "dotenv -e .env.staging -- sst remove --stage staging",
    "remove:production": "dotenv -e .env.production -- sst remove --stage production",
    "typecheck": "tsc --noEmit"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@types/node": "^20.11.0",
    "dotenv-cli": "^11.0.0",
    "sst": "^3.3.27",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.700.0",
    "@aws-sdk/client-mediaconvert": "^3.700.0",
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/client-sqs": "^3.700.0",
    "@aws-sdk/client-transcribe": "^3.700.0"
  }
}
